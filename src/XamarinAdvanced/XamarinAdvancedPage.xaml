<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:XamarinAdvanced"
    x:Class="XamarinAdvanced.XamarinAdvancedPage"
    xmlns:vm="clr-namespace:XamarinAdvanced.ViewModel">
    <ContentPage.BindingContext>
        <vm:XamarinAdvancedViewModel />
    </ContentPage.BindingContext>
    <StackLayout>
        <Button Text="DataTemplate Selector" Command="{Binding GoToDataTemplateCommand}" />
        <Button Text="Themes" Command="{Binding GoToThemesCommand}" />
        <Button Text="Behaviors" Command="{Binding GoToBehaviorsCommand}" />
        <Button Text="Triggers" Command="{Binding GoToTriggersCommand}" />
        <Button Text="Renderers - Effects" Command="{Binding GoToRenderersEffectsCommand}" />
        <Button Text="Dependency Service" Command="{Binding GoToDependencyServiceCommand}" />
    </StackLayout>
</ContentPage>
